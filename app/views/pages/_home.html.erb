<div class="jumbotron center">
	<section class="section-white">
  <div class="container">

    <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
      <!-- Indicators -->
      <ol class="carousel-indicators">
        <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
        <li data-target="#carousel-example-generic" data-slide-to="1"></li>
        <li data-target="#carousel-example-generic" data-slide-to="2"></li>
      </ol>

      <!-- Wrapper for slides -->
      <div class="carousel-inner">
        <div class="item active">
          <img src="http://brightondogphotography.co.uk/wp-content/uploads/2012/07/dog-friendly-brighton-pub-1.jpg" alt="puginapub">
          <div class="carousel-caption">
            <h2>collecting great pics of old pubs</h2>
          </div>
        </div>
        <div class="item">
          <img src="http://theicecreambarsf.com/home/home_img1.jpg" alt="icecreambar">
          <div class="carousel-caption">
            <h2>and all kinds of bars</h2>
          </div>
        </div>
        <div class="item">
          <img src="https://almejeiras.files.wordpress.com/2012/05/hemingway-9.jpg" alt="Hemingway">
          <div class="carousel-caption">
            <h2>... or just whatever</h2>
          </div>
        </div>
      </div>

      <!-- Controls -->
      <a class="left carousel-control" href="#carousel-example-generic" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
      </a>
      <a class="right carousel-control" href="#carousel-example-generic" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
      </a>
    </div>

  </div>
</section>
	<h1>Welcome to BrewPinting</h1>
      <p>
        <%= link_to "Log in", new_user_session_path, class: "btn btn-default btn-lg" %>
        <%= link_to "Sign up", new_user_registration_path, class: "btn btn-primary btn-lg" %>
      </p>
</div>
